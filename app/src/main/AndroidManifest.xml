<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ad.sukasukanginep">

    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainNavigasiActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LogReg.LoginActivity"
            android:label="@string/title_login"></activity>
        <activity android:name=".LogReg.RegisterActivity"
            android:label="@string/title_register"></activity>
        <activity android:name=".Profile.ProfilActivity"
            android:label="@string/title_activity_profile"></activity>
        <activity android:name=".Profile.EditProfilActivity"
            android:label="@string/title_editProf"></activity>
        <activity android:name=".Profile.PendaftaranRumah"
            android:label="@string/title_daftarHost"></activity>
    </application>

</manifest>